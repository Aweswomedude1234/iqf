{
  "version": 2,
  "buildCommand": "npm run build:vercel",
  "outputDirectory": "dist/public",
  "installCommand": "npm install",
  "framework": null,
  "functions": {
    "api/events.ts": {
      "runtime": "@vercel/node"
    },
    "api/events/[id].ts": {
      "runtime": "@vercel/node"
    },
    "api/team.ts": {
      "runtime": "@vercel/node"
    },
    "api/team/[id].ts": {
      "runtime": "@vercel/node"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}